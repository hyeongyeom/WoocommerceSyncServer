{
  "compilerOptions": {
    "lib": [
      "es5",
      "es6"
    ],
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./build",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "sourceMap": true
  },
  "include": [
    "*.ts",
    "./src/**/*",
    "__test__/sqs.test.ts",
    "*.d.ts"
  ],
  "exclude": [
    "node_modules",
    "tests"
  ]
}