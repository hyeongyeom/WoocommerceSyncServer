{
  "name": "woocommerce-sync-server",
  "version": "0.0.1",
  "description": "데이터를 wordpress 플러그인 woocommerce와 mongodb 간 데이터 전송 및 수신 프로젝트입니다.",
  "main": "app.js",
  "repository": "",
  "author": "gyeom",
  "license": "MIT",
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "scripts": {
    "setDockerfile:dev": "rimraf Dockerfile && cpy Dockerfile.dev . --rename=Dockerfile",
    "setDockerfile:staging": "rimraf Dockerfile && cpy Dockerfile.staging . --rename=Dockerfile",
    "setDockerfile:prod": "rimraf Dockerfile && cpy Dockerfile.prod . --rename=Dockerfile",
    "start": "node ./build/app.js",
    "dev": "cross-env NODE_ENV=development PORT=80 ts-node src/app.ts",
    "staging": "cross-env NODE_ENV=staging PORT=80 ts-node src/app.ts",
    "prod": "cross-env NODE_ENV=production PORT=80 ts-node src/app.ts",
    "dev:debug:prev": "cross-env NODE_ENV=development PORT=80 node --require ts-node/register src/app.ts",
    "build": "tsc -p .",
    "deploy": "yarn build && yarn gcpdeploy",
    "gcpdeploy": "gcloud app deploy",
    "dd": "nodemon --exec ts-node src/routes/module/dataParsing.ts",
    "test": "cross-env NODE_ENV=development jest --detectOpenHandles",
    "eb-create:dev": "npm run setDockerfile:dev && eb create",
    "eb-create:staging": "npm run setDockerfile:staging && eb create",
    "eb-create:prod": "npm run setDockerfile:prod && eb create",
    "eb-deploy:dev": "npm run setDockerfile:dev && eb deploy test-dev3",
    "eb-deploy:staging": "npm run setDockerfile:staging && eb deploy WcSyncApp-env",
    "eb-deploy:prod": "npm run setDockerfile:prod && eb deploy WcSyncApp-env"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "^3.3.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.20",
    "cross-env": "^7.0.3",
    "frisby": "^2.1.3",
    "jest": "^26.6.3",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.5.3",
    "ts-node": "^8.10.2",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^3.0.6",
    "@types/express": "^4.17.2",
    "@woocommerce/woocommerce-rest-api": "^1.0.1",
    "acf": "^0.2.1",
    "agenda": "^3.1.0",
    "aws-sdk": "^2.850.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "celebrate": "^12.1.0",
    "connect-redis": "^5.2.0",
    "cors": "^2.8.5",
    "delay": "^5.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "fastq": "^1.11.0",
    "lotus": "^1.0.1",
    "moment": "^2.29.1",
    "mongodb": "^3.6.3",
    "mongoose": "^5.10.0",
    "mongoose-type-url": "^2.1.0",
    "p-event": "^4.2.0",
    "p-queue": "^6.6.2",
    "redis": "^3.1.2",
    "sqs-bulk-loader": "^2.15.0",
    "sqs-consumer": "^5.5.0",
    "typedi": "^0.8.0",
    "util": "^0.12.3",
    "winston": "^3.3.3",
    "winston-cloudwatch": "^2.5.0"
  }
}